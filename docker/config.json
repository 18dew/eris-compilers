{
	"lll": {
		"url": "https://compilers.eris.industries:9090/compile",
		"net": true,
		"extensions": [
			"lll",
			"def"
		],
		"regexes": [
			"\\(include \"(.+?)\"\\)"
		],
		"replaces": [
			[
				"(include \"",
				".lll\")"
			]
		],
		"cmd": [
			"lllc",
			"_"
		],
		"abi": null
	},
	"se": {
		"url": "https://compilers.eris.industries:9090/compile",
		"net": true,
		"extensions": [
			"se"
		],
		"regexes": [
			"create\\(\"(.+?)\"\\)",
			"create\\('(.+?)'\\)"
		],
		"replaces": [
			[
				"create(\"",
				".se\")"
			],
			[
				"create('",
				".se')"
			]
		],
		"cmd": [
			"/usr/bin/sc",
			"compile",
			"_"
		],
		"abi": [
			"/usr/bin/sc",
			"mk_full_signature",
			"_"
		]
	},
	"sol": {
		"url": "http://localhost:9097",
		"net": true,
		"extensions": [
			"sol"
		],
		"regexes": [
			"import \"(.+?)\";"
		],
		"replaces": [
			[
				"(import \"./",
				".sol\";"
			]
		],
		"cmd": [
			"solc",
			"--bin",
			"_",
			"imports",
			"|",
			"grep",
			"Binary",
			"-A1",
			"|",
			"tail",
			"-n",
			"1"
		],
		"abi": [
			"solc",
			"--abi",
			"_",
			"imports",
			"|",
			"grep",
			"ABI",
			"-A1",
			"|",
			"tail",
			"-n",
			"1"
		]
	}
}
